<?xml version="1.0" encoding="UTF-8"?>
<!-- 
	this is Registry Cleaner
	DO NOT CHANGE THE PATH ATTRIBUTE VALUE
-->
<cleaner id="little_reg" os="windows">
  <label>Windows Registry Cleaner</label>
  <description>Registry Cleaner</description>
  <option id="activex">
    <label>ActiveX/Com</label>
    <description></description>
	<warning>This scanner may take a while to finish.</warning>
    <action command="littleregistry" search="lrc.activexcom" path="HKLM\SOFTWARE" />
  </option>
  <option id="appinfo">
    <label>Application Info</label>
    <description></description>
    <action command="littleregistry" search="lrc.appinfo" path="HKLM\SOFTWARE" />
  </option>
  <option id="progloc">
    <label>Program Locations</label>
    <description></description>
    <action command="littleregistry" search="lrc.progloc" path="HKLM\SOFTWARE" />
  </option>
  <option id="software_settings">
    <label>Software Settings</label>
    <description></description>
    <action command="littleregistry" search="lrc.software.settings" path="HKLM\SOFTWARE" />
  </option>
  <option id="startup">
    <label>Startup</label>
    <description></description>
    <action command="littleregistry" search="lrc.startup" path="HKLM\SOFTWARE" />
  </option>
  <option id="system_drivers">
    <label>System Drivers</label>
    <description></description>
    <action command="littleregistry" search="lrc.sysdrv" path="HKLM\SOFTWARE" />
  </option>
  <option id="shared_dlls">
    <label>Shared DLLs</label>
    <description></description>
    <action command="littleregistry" search="lrc.shareddll" path="HKLM\SOFTWARE" />
  </option>
  <option id="help_files">
    <label>Help Files</label>
    <description></description>
    <action command="littleregistry" search="lrc.helpfile" path="HKLM\SOFTWARE" />
  </option>
  <option id="sound_event">
    <label>Sound Events</label>
    <description></description>
    <action command="littleregistry" search="lrc.soundevent" path="HKLM\SOFTWARE" />
  </option>
  <option id="history_list">
    <label>History List</label>
    <description></description>
    <action command="littleregistry" search="lrc.histlist" path="HKLM\SOFTWARE" />
  </option>
  <option id="winfont">
    <label>Windows Fonts</label>
    <description></description>
    <action command="littleregistry" search="lrc.winfonts" path="HKLM\SOFTWARE" />
  </option>
</cleaner>